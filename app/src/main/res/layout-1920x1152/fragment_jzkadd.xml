<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:layout_width="match_parent"

    android:layout_height="match_parent">


    <RelativeLayout

        android:id="@+id/jzk1_fl01"

        android:layout_width="match_parent"

        android:layout_height="match_parent"

        android:visibility="visible">


        <com.ppfuns.jxyyzl.view.TvEditText

            android:id="@+id/jzk1_et_xm"

            android:layout_width="330px"

            android:layout_height="104px"

            android:layout_marginLeft="303px"

            android:layout_marginTop="96px"

            android:background="@drawable/edit_focus"

            android:focusable="true"

            android:gravity="center"

            android:hint="输入姓名"

            android:textColor="@color/colorText1"

            android:textSize="45px" />


        <com.ppfuns.jxyyzl.view.PopEditTextCard

            android:id="@+id/jzk1_et_ktype"

            android:layout_width="330px"

            android:layout_height="104px"

            android:layout_marginLeft="651px"

            android:layout_marginTop="96px"

            android:background="@drawable/edit_focus"

            android:gravity="center"

            android:hint="选择卡号类型"

            android:textColor="@color/colorText1"

            android:textColorHint="#000"

            android:textSize="45px" />


        <com.ppfuns.jxyyzl.view.PopEditHostpital

            android:id="@+id/jzk1_et_hs"

            android:layout_width="693px"

            android:layout_height="104px"

            android:layout_marginLeft="303px"

            android:layout_marginTop="224px"

            android:background="@drawable/edit_focus"

            android:gravity="center"

            android:hint="输入医院"

            android:textColor="@color/colorText1"

            android:textSize="45px" />


        <com.ppfuns.jxyyzl.view.TvEditText

            android:id="@+id/jzk1_et_kh"

            android:layout_width="693px"

            android:layout_height="104px"

            android:layout_marginLeft="303px"

            android:layout_marginTop="352px"

            android:background="@drawable/edit_focus"

            android:gravity="center"

            android:hint="输入就诊卡号"

            android:textColor="@color/colorText1"

            android:textSize="45px" />


        <LinearLayout

            android:layout_width="1065px"

            android:layout_height="1px"

            android:layout_marginLeft="97px"

            android:layout_marginTop="553px"

            android:background="#7DC7DF" />


        <LinearLayout

            android:layout_width="693px"

            android:layout_height="104px"

            android:layout_marginLeft="303px"

            android:layout_marginTop="624px"

            android:gravity="center"

            android:orientation="horizontal">


            <Button

                android:id="@+id/jzk1_bt_cx"

                android:layout_width="297px"

                android:layout_height="104px"

                android:gravity="center"

                android:text="取消"

                android:textColor="@color/colorText1"

                android:textSize="45px" />

            <Button

                android:id="@+id/jzk1_bt_next"

                android:layout_width="297px"

                android:layout_height="104px"

                android:layout_marginLeft="36px"

                android:gravity="center"

                android:text="下一步"

                android:textColor="@color/colorText1"

                android:textSize="45px" />


        </LinearLayout>


    </RelativeLayout>


    <RelativeLayout

        android:id="@+id/jzk2_fl02"

        android:layout_width="match_parent"

        android:layout_height="match_parent"

        android:visibility="gone">


        <TextView

            android:id="@+id/jzk2_tv_xm"

            android:layout_width="255px"

            android:layout_height="104px"

            android:layout_marginLeft="187px"

            android:layout_marginTop="96px"

            android:background="@drawable/enablefalse"

            android:enabled="false"

            android:gravity="center"

            android:hint="预约人姓名"

            android:singleLine="true"

            android:textColor="@color/colorText1"

            android:textColorHint="#000"

            android:textSize="45px" />


        <LinearLayout

            android:layout_width="270px"

            android:layout_height="104px"

            android:layout_marginLeft="457px"

            android:layout_marginTop="96px"

            android:background="@drawable/edit_focus"

            android:gravity="center"

            android:orientation="horizontal">


            <CheckBox

                android:id="@+id/jzk2_ck01"

                android:layout_width="match_parent"

                android:layout_height="match_parent"

                android:text="是否儿童"

                android:textColor="#000"

                android:textSize="45px" />

        </LinearLayout>


        <com.ppfuns.jxyyzl.view.TvEditText

            android:id="@+id/jzk2_et_xm2"

            android:layout_width="345px"

            android:layout_height="104px"

            android:layout_marginLeft="750px"

            android:layout_marginTop="96px"

            android:background="@drawable/edit_focus"

            android:gravity="center"

            android:hint="输入监护人姓名"

            android:singleLine="true"

            android:textColor="@color/colorText1"

            android:textSize="45px" />


        <com.ppfuns.jxyyzl.view.PopEditTextSex

            android:id="@+id/jzk2_et_sex"

            android:layout_width="382px"

            android:layout_height="104px"

            android:layout_marginLeft="187px"

            android:layout_marginTop="224px"

            android:background="@drawable/edit_focus"

            android:enabled="false"

            android:gravity="center"

            android:hint="选择性别"

            android:singleLine="true"

            android:textColor="@color/colorText1"

            android:textColorHint="#000"

            android:textSize="45px" />


        <com.ppfuns.jxyyzl.view.TvEditText

            android:id="@+id/jzk2_et_sfz"

            android:layout_width="495px"

            android:layout_height="104px"

            android:layout_marginLeft="600px"

            android:layout_marginTop="224px"

            android:background="@drawable/edit_focus"

            android:gravity="center"

            android:hint="输入监护人身份证号"

            android:singleLine="true"

            android:textColor="@color/colorText1"

            android:textSize="45px" />


        <Button

            android:id="@+id/jzk2_bt_sr"

            android:layout_width="382px"

            android:layout_height="104px"

            android:layout_marginLeft="187px"

            android:layout_marginTop="352px"

            android:background="@drawable/edit_focus"

            android:gravity="center"

            android:hint="出生日期"

            android:singleLine="true"

            android:textColor="@color/colorText1"

            android:textColorHint="#000"

            android:textSize="45px" />


        <com.ppfuns.jxyyzl.view.TvEditText

            android:id="@+id/jzk2_et_ph"

            android:layout_width="495px"

            android:layout_height="104px"

            android:layout_marginLeft="600px"

            android:layout_marginTop="352px"

            android:background="@drawable/edit_focus"

            android:gravity="center"

            android:hint="输入监护人手机号码"

            android:singleLine="true"

            android:textColor="@color/colorText1"

            android:textSize="45px" />


        <LinearLayout

            android:layout_width="1065px"

            android:layout_height="1px"

            android:layout_marginLeft="97px"

            android:layout_marginTop="553px"

            android:background="#7DC7DF" />


        <LinearLayout

            android:layout_width="693px"

            android:layout_height="104px"

            android:layout_marginLeft="318px"

            android:layout_marginTop="624px"

            android:gravity="center"

            android:orientation="horizontal">


            <Button

                android:id="@+id/jzk2_bt_cx2"

                android:layout_width="297px"

                android:layout_height="104px"

                android:gravity="center"

                android:text="取消"

                android:textColor="@color/colorText1"

                android:textSize="45px" />


            <Button

                android:id="@+id/jzk2_bt_tj"

                android:layout_width="297px"

                android:layout_height="104px"

                android:layout_marginLeft="36px"

                android:gravity="center"

                android:text="提交"

                android:textColor="@color/colorText1"

                android:textSize="45px" />

        </LinearLayout>

    </RelativeLayout>

</RelativeLayout>

