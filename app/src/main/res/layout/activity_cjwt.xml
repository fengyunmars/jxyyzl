<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/cjwtbak"
    android:focusableInTouchMode="false">

    <com.ppfuns.jxyyzl.view.DateView
        android:layout_width="215px"
        android:layout_height="80px"
        android:layout_marginLeft="1005px"
        android:layout_marginTop="28px"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="right"
        android:textColor="#FFFFFF"
        android:textSize="24px" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="334px"
        android:layout_marginTop="125px"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="left|center">

        <ImageView
            android:layout_width="33px"
            android:layout_height="33px"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:src="@drawable/num1y" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:text="常见问题答疑"
            android:textColor="@color/colorText1"
            android:textSize="28px" />
    </LinearLayout>


    <com.ppfuns.jxyyzl.view.ListViewTv
        android:id="@+id/yygh_lv01"
        android:layout_width="218px"
        android:layout_height="488px"
        android:layout_marginLeft="64px"
        android:layout_marginTop="190px"
        android:divider="#00000000"
        android:dividerHeight="0dip"
        android:listSelector="@drawable/list_focus"
        android:scrollbars="vertical" />

    <android.support.v17.leanback.widget.VerticalGridView
        android:id="@+id/jzrgladd_fl01"
        android:layout_width="888px"
        android:layout_height="488px"
        android:layout_marginLeft="318px"
        android:layout_marginTop="190px"
        android:focusable="false"
        android:focusableInTouchMode="false" />

</RelativeLayout>
